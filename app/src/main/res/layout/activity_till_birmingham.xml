                    android:textColor="#ffffff"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="left" />

                <TextView
                    android:layout_width="180dp"
                    android:layout_height="20dp"
                    android:id="@+id/footlongcstaffcount"
                    android:text=""
                    android:textSize="18sp"
                    android:ems="10"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="left"/>
                <TextView
                    android:layout_width="180dp"
                    android:layout_height="20dp"
                    android:id="@+id/specialcstaffcount"
                    android:text=""
                    android:textSize="18sp"
                    android:ems="10"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="left" />
                <TextView
                    android:layout_width="180dp"
                    android:layout_height="20dp"
                    android:id="@+id/chickencstaffcount"
                    android:text=""
                    android:textSize="18sp"
                    android:ems="10"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:gravity="left" />

                <TextView
                    android:layout_width="180dp"
                    android:layout_height="20dp"
                    android:id="@+id/staffstaffcount"
                    android:text=""
                    android:textSize="18sp"
                    android:ems="10"
                    android:textColor="#ffffff"
                    android:layout_marginLeft="10dp"
